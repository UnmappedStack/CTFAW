const globalthing: u64 = 21 + 3 * 2;

func main() {
    let msg: u64 = "hello world!\n";
    let num: u64 = 5;
    // write the text to stdout
    asm("mov rax, 1\nmov rdi, 1\nmov rdx, 13\nsyscall" : "rsi" | msg : : );
    let global_loc: u64 = &globalthing;
    return *global_loc + 42;
}

func _start() {
    let status: u64 = main();
    asm("mov rax, 60\nsyscall" : "rdi" | status : :);
}
